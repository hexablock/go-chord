namespace fbtypes;

table Vnode {
    Id:[ubyte];
    Host: string;
}

table VnodePair {
    Target: Vnode;
    Self: Vnode;
}

table VnodeIntBytes {
    Vnode: Vnode;
    Int: int;
    Bytes:[ubyte];
}

table VnodeListErr {
    Err: string;
    Vnodes: [Vnode];
}

table VnodeErr {
    Vnode: Vnode;
    Err: string;
}

table BoolErr {
    Err: string;
    Bool: bool;
}

table String {
    String: string;
}
